{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "QuizSubmitResponse",
  "type": "object",
  "required": ["score", "correct", "total"],
  "properties": {
    "score": {"type": "integer", "minimum": 0, "maximum": 15},
    "correct": {"type": "array", "items": {"type": "string"}, "minItems": 15, "maxItems": 15},
    "total": {"type": "integer", "const": 15}
  },
  "additionalProperties": false
}